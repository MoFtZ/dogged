{
  "metadata": [
    {
      "src": [
        {
          "cwd": "../",
          "files": [
            "Dogged/**.csproj"
          ]
        }
      ],
      "dest": "api/dogged",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "cwd": "../",
          "files": [
            "Dogged.Native/**.csproj"
          ]
        }
      ],
      "dest": "api/dogged.native",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/dogged/**.yml",
          "api/dogged.native/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "examples/**.md",
          "examples/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "template"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
